{
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 100,
	"singleQuote": true,
	"semi": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"proseWrap": "preserve",
	"overrides": [{ "files": ["*.yml", "*.yaml"], "options": { "tabWidth": 2, "useTabs": true } }]
}
